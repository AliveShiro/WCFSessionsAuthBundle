{
  "name": "xanily/wcf-sessions-auth-bundle",
  "type": "symfony-bundle",
  "description": "A symfony bundle that provides symfony the ability to integrate and share sessions with WCF and use WCF as the authentication provider.",
  "keywords": [
    "WCF",
    "intergration",
    "bridge"
  ],
  "version": "2.1",
  "homepage": "https://www.faldoria.de",
  "license": "MIT",
  "authors": [
    {
      "name": "xanily",
      "email": "xanily@faldoria.de",
      "homepage": "https://www.faldoria.de",
      "role": "Developer"
    }
  ],
  "require": {
    "symfony/framework-bundle": "^4.0|^4.2",
    "symfony/twig-bundle": "^4.0|^4.2",
    "symfony/form": "^4.0|^4.2",
    "symfony/security-bundle": "^4.0|^4.2",
    "doctrine/common": "~2.10",
    "doctrine/orm": "~2.6",
    "symfony/doctrine-bridge" : "^4.0|^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "symfony/var-dumper": "*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {"xanily\\WCFSessionsAuthBundle\\": "src/"}
  }
}